{
  "name": "mongocamp-server",
  "version": "1.1.0",
  "description": "",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs --port 5555",
    "api-docs-codegen": "openapi-generator-cli generate -i http://0.0.0.0:8080/docs/docs.yaml -g markdown -o ./docs/rest",
    "api-codegen": "openapi-generator-cli generate -i http://0.0.0.0:8080/docs/docs.yaml -o ./src/test/scala/dev/mongocamp/server/client-project -g scala-sttp --additional-properties=mainPackage=dev.mongocamp.server.client,jsonLibrary=circe,dateLibrary=joda"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MongoCamp/mongocamp-server.git"
  },

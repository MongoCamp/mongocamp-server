  "bugs": {
    "url": "https://github.com/MongoCamp/mongocamp-server/issues"
  },
  "homepage": "https://github.com/MongoCamp/mongocamp-server#readme",
  "devDependencies": {
    "vitepress": "^1.0.0-alpha.2",
    "@openapitools/openapi-generator-cli": "^2.4.26"
  }
}
